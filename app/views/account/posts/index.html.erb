<h2>My Posts</h2>
<table class="table table-bordered">
	<thead>
		<tr>
			<th>#</th>
			<th>Content</th>
			<th>Last Update</th>
		</tr>
	</thead>
	<tbody>
		<% @posts.each do |post| %>
			<tr>
				<td><%= post.group.id %></td>
				<td><%= post.content %></td>
				<td><%= post.updated_at.to_s(:long) %></td>
			</tr>
		<% end %>
	</tbody>
</table>